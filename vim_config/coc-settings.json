{
"languageserver": {
  "rust": {
    "command": "rust-analyzer",
    "filetypes": ["rust"],
    "rootPatterns": ["Cargo.toml"]
  },
  "go": {
   "command": "gopls",
   "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
   "trace.server": "verbose",
   "filetypes": ["go"],
   "initializationOptions": {
	"usePlaceholders": true
   }
  }
}
